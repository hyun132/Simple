<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/mapsFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.example.testathome.ui.fragments.SearchFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_homeFragment_to_mapsFragment"
            app:destination="@id/mapsFragment" />
        <action
            android:id="@+id/action_homeFragment_to_savedFragment"
            app:destination="@id/savedFragment" />
    </fragment>
    <fragment
        android:id="@+id/mapsFragment"
        android:name="com.example.testathome.ui.map.MapsFragment"
        android:label="fragment_maps"
        tools:layout="@layout/fragment_maps">
        <action
            android:id="@+id/action_mapsFragment_to_homeFragment"
            app:destination="@id/homeFragment"
            app:popUpTo="@id/savedFragment"
            app:popUpToInclusive="true"/>
        <argument
            android:name="RestaurnatItem"
            app:argType="com.example.testathome.models.Item" />
        <action
            android:id="@+id/action_mapsFragment_to_savedFragment"
            app:destination="@id/savedFragment"
            app:popUpTo="@id/savedFragment"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/savedFragment"
        android:name="com.example.testathome.ui.mylist.SavedFragment"
        android:label="SavedFragment" >
        <action
            android:id="@+id/action_savedFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
        <action
            android:id="@+id/action_savedFragment_to_dialogFragment"
            app:destination="@id/dialogFragment" />
    </fragment>
    <fragment
        android:id="@+id/dialogFragment"
        android:name="com.example.testathome.ui.DialogFragment"
        android:label="DialogFragment" />
    <fragment
        android:id="@+id/thridFragment"
        android:name="com.example.testathome.ui.setting.SettingFragment"
        android:label="fragment_thrid"
        tools:layout="@layout/fragment_setting" />
</navigation>